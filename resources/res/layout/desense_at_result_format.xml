<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <View android:background="#ff909090" android:layout_width="match_parent" android:layout_height="1dp"/>
    <TextView android:textSize="16sp" android:gravity="center_horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:layout_marginBottom="10dp" android:text="@string/desense_at_result_title"/>
    <View android:background="#ff909090" android:layout_width="match_parent" android:layout_height="1dp"/>
    <LinearLayout android:padding="10dp" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:gravity="center" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/desense_at_result_title_item" android:layout_weight="1"/>
        <TextView android:gravity="center" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/desense_at_result_title_result" android:layout_weight="1"/>
        <LinearLayout android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="5">
            <TextView android:gravity="center_horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/desense_at_result_title_desense"/>
            <LinearLayout android:id="@+id/desense_at_sub_result" android:layout_width="match_parent" android:layout_height="wrap_content">
                <TextView android:gravity="center_horizontal" android:id="@+id/desense_at_result_title_gps" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/desense_at_gps" android:layout_weight="1"/>
                <TextView android:gravity="center_horizontal" android:id="@+id/desense_at_result_title_gps_l5" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/desense_at_result_gps_l5" android:layout_weight="1"/>
                <TextView android:gravity="center_horizontal" android:id="@+id/desense_at_result_title_gl1" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/desense_at_result_gl1" android:layout_weight="1"/>
                <TextView android:gravity="center_horizontal" android:id="@+id/desense_at_result_title_gl2" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/desense_at_result_gl2" android:layout_weight="1"/>
                <TextView android:gravity="center_horizontal" android:id="@+id/desense_at_result_title_gl3" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/desense_at_result_gl3" android:layout_weight="1"/>
                <TextView android:gravity="center_horizontal" android:id="@+id/desense_at_result_title_bd" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/desense_at_result_bd" android:layout_weight="1"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <View android:background="#ff909090" android:layout_width="match_parent" android:layout_height="1dp"/>
    <ListView android:id="@+id/desense_at_test_result" android:layout_width="match_parent" android:layout_height="match_parent"/>
</LinearLayout>
