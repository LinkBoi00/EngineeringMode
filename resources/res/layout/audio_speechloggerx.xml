<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <TableLayout android:id="@+id/TableLayout01" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="3pt" android:layout_marginTop="3pt" android:text="@string/Audio_SpeechLogger_FileText"/>
            <TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <CheckBox android:id="@+id/Audio_SpeechLogger_Enable" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/Audio_SpeechLogger_Enable"/>
            </TableRow>
            <TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <RadioGroup android:id="@+id/RadioGroup1" android:layout_marginLeft="60dp">
                    <RadioButton android:id="@+id/Audio_SpeechLogger_EPL" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/Audio_Enable_EPL"/>
                    <RadioButton android:id="@+id/Audio_SpeechLogger_Normalvm" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/Audio_Enable_Normalvm"/>
                </RadioGroup>
            </TableRow>
            <View android:background="#ff909090" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginTop="5pt"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="3pt" android:layout_marginTop="3pt" android:text="@string/Audio_EPLDebug_FileText"/>
            <TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <CheckBox android:id="@+id/Audio_EPLDebug_Enable" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/Audio_EPLDebug_Enable"/>
            </TableRow>
            <View android:background="#ff909090" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginTop="5pt"/>
            <TextView android:id="@+id/Audio_CTM4WAYLogger_EnableText" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="3pt" android:layout_marginTop="3pt" android:text="@string/Audio_CTM4WAYLogger_EnableText"/>
            <TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <CheckBox android:id="@+id/Audio_CTM4WAYLogger_Enable" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/Audio_CTM4WAYLogger_Enable"/>
            </TableRow>
            <View android:id="@+id/Audio_View1" android:background="#ff909090" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginTop="5pt"/>
            <TextView android:id="@+id/Audio_AncLogger_FileText" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="3pt" android:layout_marginTop="3pt" android:text="@string/Audio_AncLogger_FileText"/>
            <TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <CheckBox android:id="@+id/Audio_AncLogger_Enable" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/Audio_AncLogger_Enable"/>
            </TableRow>
            <TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <RadioGroup android:id="@+id/RadioGroup2" android:layout_marginLeft="60dp">
                    <RadioButton android:id="@+id/Audio_AncLogger_Down" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/Audio_Enable_Down"/>
                    <RadioButton android:id="@+id/Audio_AncLogger_NoDown" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/Audio_Enable_NoDown"/>
                </RadioGroup>
            </TableRow>
            <View android:id="@+id/Audio_View3" android:background="#ff909090" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginTop="5pt"/>
            <TextView android:id="@+id/Audio_MagiConference_EnableText" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="3pt" android:layout_marginTop="3pt" android:text="@string/Audio_MagiConference_EnableText"/>
            <TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <CheckBox android:id="@+id/Audio_MagiConference_Enable" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/Audio_MagiConference_Enable"/>
            </TableRow>
        </TableLayout>
        <View android:id="@+id/Audio_View2" android:background="#ff909090" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginTop="5pt"/>
        <Button android:id="@+id/Dump_Speech_DbgInfo" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="5pt" android:text="@string/Audio_Dump_SpeechDbgInfo"/>
        <View android:id="@+id/Audio_View2" android:background="#ff909090" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginTop="5pt"/>
        <TextView android:id="@+id/Audio_hal_dump" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="3pt" android:layout_marginTop="3pt" android:text="@string/Audio_hal_dump"/>
        <RelativeLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
            <Button android:id="@+id/Audio_move_to_sdcard" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="5pt" android:text="@string/Audio_move_to_sdcard" android:layout_alignParentStart="true"/>
            <Button android:id="@+id/Audio_clear_dump" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="15pt" android:layout_marginTop="5pt" android:text="@string/Audio_clear_dump" android:layout_toRightOf="@+id/Audio_move_to_sdcard"/>
        </RelativeLayout>
        <TextView android:id="@+id/Audio_copy_progress" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="3pt" android:layout_marginTop="3pt"/>
    </LinearLayout>
</ScrollView>
