    <LinearLayout android:padding="10dp" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:gravity="center" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/clk_quality_at_result_title_item" android:layout_weight="1"/>
