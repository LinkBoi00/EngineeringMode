<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:custom="http://schemas.android.com/apk/res-auto" android:id="@+id/memory_flash_tabs" android:layout_width="match_parent" android:layout_height="match_parent">
    <ScrollView android:id="@+id/file_sys_view" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:id="@+id/file_sys_info" android:scrollbars="vertical" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    </ScrollView>
    <ScrollView android:id="@+id/partition_view" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:id="@+id/partition_info" android:scrollbars="vertical" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    </ScrollView>
</FrameLayout>
