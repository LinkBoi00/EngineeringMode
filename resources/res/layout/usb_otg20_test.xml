                <Button android:id="@+id/USB_IF_OTG20_Elec_TEST_PACKET_Start_ID" android:layout_width="100dp" android:layout_height="wrap_content" android:text="@string/USB_IF_Text_START"/>
                <Button android:id="@+id/USB_IF_OTG20_Elec_TEST_PACKET_Stop_ID" android:layout_width="100dp" android:layout_height="wrap_content" android:text="@string/USB_IF_Text_STOP"/>
            </TableRow>
        </TableLayout>
    </LinearLayout>
</ScrollView>
